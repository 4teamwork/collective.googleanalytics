<tal:has_data condition="view/data">
    <div tal:content="structure view/introduction"></div>
    <div tal:define="height view/visualization/height;" 
        tal:attributes="id view/visualization/id;
            style python:(height and 'height:' + str(height) + 'px;;') or ''"></div>
    <script type="text/javascript" tal:content="view/visualization/javascript"></script>
    <div tal:content="structure view/conclusion"></div>
</tal:has_data>
<tal:no_data condition="not:view/data">
    <p class="analytics-no-results">
        The report <strong tal:content="view/report/title">Report Title</strong>
        did not return any results.
    </p>
</tal:no_data>

